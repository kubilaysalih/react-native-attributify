{
  "name": "react-native-attributify",
  "version": "0.1.0-alpha.5",
  "description": "react-native-attributify like nativewind and unocss with attributes",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "author": "kubilay salih",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "require": "./dist/presets/index.js"
    },
    "./presets/native": {
      "types": "./dist/types/presets/native/index.d.ts",
      "default": "./dist/presets/native/index.js"
    },
    "./presets/layout": {
      "types": "./dist/types/presets/layout/index.d.ts",
      "default": "./dist/presets/layout/index.js"
    },
    "./babel": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubilaysalih/react-native-attributify.git"
  },
  "bugs": {
    "url": "https://github.com/kubilaysalih/react-native-attributify/issues"
  },
  "homepage": "https://github.com/kubilaysalih/react-native-attributify#readme",
  "keywords": [
    "react-native",
    "styling",
    "utility",
    "css-in-js",
    "atomic-css",
    "typescript"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/types": "^7.26.3",
    "@eslint/js": "^9.17.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/__tests__"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
